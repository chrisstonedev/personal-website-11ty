{% extends "base.njk" %}

{% block main %}
    {{ content | safe }}
    <ul>
        {%- for post in collections.blog -%}
            <li>
                <h2><a href="{{ post.url }}">{{ post.data.title }}</a></h2>
                {{ post.data.content | firstParagraph | safe }}
            </li>
        {%- endfor -%}
    </ul>
{% endblock %}